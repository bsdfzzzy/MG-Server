{
  "name": "server",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "./node_modules/.bin/nodemon bin/run",
    "koa": "./node_modules/.bin/runkoa bin/www",
    "pm2": "pm2 start bin/run ",
    "test": "nyc ava",
    "build": "BABEL_ENV=production babel --out-of=dist index.js"
  },
  "dependencies": {
    "co": "^4.6.0",
    "debug": "^2.2.0",
    "jade": "~1.11.0",
    "koa": "^2.0.0",
    "koa-bodyparser": "^2.0.1",
    "koa-convert": "^1.2.0",
    "koa-json": "^1.1.1",
    "koa-logger": "^1.3.0",
    "koa-onerror": "^1.2.1",
    "koa-router": "^7.0.0",
    "koa-static": "^2.0.0",
    "koa-views": "^5.0.1",
    "runkoa": "^1.3.1"
  },
  "devDependencies": {
    "ava": "^0.15.2",
    "babel-core": "^6.11.4",
    "babel-preset-stage-2": "^6.11.0",
    "babel-register": "^6.11.5",
    "gulp": "^3.9.1",
    "gulp-ava": "^0.12.1",
    "nodemon": "^1.8.1",
    "nyc": "^7.1.0",
    "supertest": "^1.2.0"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2"
    ],
    "env": {
      "development": {
        "sourceMaps": "inline"
      }
    }
  },
  "ava": {
    "files": [
      "test/**/*.js",
      "test/*.js"
    ],
    "source": [
      "**/*.js"
    ],
    "failFast": true,
    "tap": true,
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  }
}
